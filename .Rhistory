geom_bar(stat = "identity", fill = "grey") +
labs(title = titulo,
subtitle = subtitulo,
x = x_label,
y = y_label)  +
geom_text(aes(label = cantidad_titulos),
vjust = -0.5,
color = "black") +
geom_hline(yintercept = promedio_titulos,
linetype = "dashed", color = "red",
linewidth = 0.5) +
theme_classic()
plot(titulos_semanales)
